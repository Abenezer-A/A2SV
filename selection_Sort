#User function Template for python3

class Solution: 
    def selectionSort(self, arr,n):
        #code here
        for i in range(n):
            minp = i
            for j in range(i, n):
                if arr[j] < arr[minp]:
                    minp = j
            arr[i], arr[minp] = arr[minp], arr[i]
                    
